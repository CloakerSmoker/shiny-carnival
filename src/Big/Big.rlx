    static BigNumber* MultiplyBySingleDigit(BigNumber* Left, i64 Right, BigNumber* Result) {
        Result->Reserve(Left->DigitCount + 1)

        i64 Carry := 0
        i64 Remainder := 0

        for (i32 Index := 0, Index < Left->DigitCount, Index++) {
            i64 Product := (Left->Digits[Index] * Right) + Carry

            Carry := self:DivMod(Product, BIG_NUMBER_BASE, &Remainder)

            Result->Digits[Index] := Remainder As i32
        }

        while (Carry) {
            i64 Quotient := self:DivMod(Carry, BIG_NUMBER_BASE, &Remainder)

            Result->Digits[Index++] := Remainder As i32
            Carry := Quotient
        }

        Result->DigitCount := Index

        return Result->Shrink()
    }

    static BigNumber* MultiplyBySingleDigit(BigNumber* Left, i64 Right) {
        BigNumber* Result := self:WithDigits(Left->DigitCount + 1)

        self:MultiplyBySingleDigit(Left, Right, Result)

        return Result
    }

