    static Primality IsPrimeShortcut(BigNumber* N) {
        i8 Confident := true
        MillerRabinShortcut* BestShortcut := null

        i32 Count := MILLER_RABIN_SHORTCUTS->Count()

        for (i32 Index := 0, Index < Count, Index++) {
            MillerRabinShortcut* Shortcut := MILLER_RABIN_SHORTCUTS->At(Index)

            if (N->IsLessThan(Shortcut->Limit)) {
                BestShortcut := Shortcut
                break
            }
        }

        if (BestShortcut = null) {
            BestShortcut := MILLER_RABIN_SHORTCUTS->At(Count - 1)
            Confident := false
        }

        if (self:MillerRabinTest(N, BestShortcut->Bases, BestShortcut->BaseCount)) {
            if (Confident) {
                return Primality::Prime
            }
            else {
                return Primality::NotDefinitelyComposite
            }
        }

        return Primality::Composite
    }

    static i8 IsPrime(BigNumber* N) {
        Primality Basic := self:IsBasicPrime(N)

        if (Basic = Primality::Prime) {
            return true
        }
        else if (Basic = Primality::Composite) {
            return false
        }

        Primality Shortcut := self:IsPrimeShortcut(N)

        if (Shortcut = Primality::Prime) {
            return true
        }
        else if (Shortcut = Primality::Composite) {
            return false
        }

        if (self:DeterministicMillerRabinTest(N)) {
            return true
        }

        return false
    }

    static BigNumber* GreatestCommonDivisor(BigNumber* A, BigNumber* B) {
        while (B->IsNotZero()) {
            BigNumber* Temp := A->Remainder(B->Reference())
            A := B
            B := Temp
        }

        return A
    }

    static BigNumber* EulerTotient(BigNumber* N) {
        BigNumber* Result := new self(N)
        BigNumber* Index := self:Constant(2)

        loop {
            if (Index->Reference()->Square()->IsGreaterThan(N)) {
                break
            }

            if (N->DivisibleBy(Index)) {
                while (N->DivisibleBy(Index)) {
                    N := N->Quotient(Index->Reference())
                }

                Result := Result->Subtract(Result->Reference()->Quotient(Index->Reference()))
            }

            Index := Index->Add(self:One())
        }

        if (N->IsGreaterThan(self:One())) {
            Result := Result->Subtract(Result->Reference()->Quotient(N))
        }

        return Result
    }

    static BigNumber* LeastCommonMultiple(BigNumber* A, BigNumber* B) {
        return A->Reference()->Multiply(B)->Quotient(self:GreatestCommonDivisor(A, B))
    }

    static BigNumber* ModularInverse(BigNumber* N, BigNumber* M) {
        BigNumber* T := self:Zero()
        BigNumber* NewT := self:One()

        BigNumber* R := M->Reference()
        BigNumber* NewR := N->Reference()

        while (NewR->IsNotZero()) {
            Print("Before: T: %bn-base-10, NewT: %bn-base-10, R: %bn-base-10, NewR: %bn-base-10\n", T, NewT, R, NewR)

            BigNumber* Quotient := R->Reference()->Quotient(NewR->Reference())

            Print("Quotient: %bn-base-10\n", Quotient)

            BigNumber* Temp := T
            T := NewT->Reference()
            NewT := Temp->Subtract(Quotient->Reference()->Multiply(NewT))
            
            BigNumber* Temp := R
            R := NewR->Reference()
            NewR := Temp->Subtract(Quotient->Multiply(NewR))

            Print("After: T: %bn-base-10, NewT: %bn-base-10, R: %bn-base-10, NewR: %bn-base-10\n", T, NewT, R, NewR)
        }

        if (T->IsZero()) {
            T := T->Add(M)
        }

        NewT->Release()
        R->Release()
        NewR->Release()

        N->Release()
        M->Release()

        return T
    }
    

