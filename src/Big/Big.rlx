    static BigNumber* FastMultiplyBySingleDigit(BigNumber* Left, i64 Right, BigNumber* Result) asm {
        push, rbp
        mov, rbp, rsp

        xor, rax, rax ; Carry

        mov, ebx, {p1 + BigNumber.DigitCount} As i32* ; Left->DigitCount
        xor, rcx, rcx

        mov, r15, p3 ; preserve result

        mov, r8, {p1 + BigNumber.Digits} As i64* ; Left->Digits
        mov, r9, {p3 + BigNumber.Digits} As i64* ; Result->Digits
        
        mov, r11, 16777216

    EachDigit:
        mov, r10d, {r8[rcx * 4]} As i32*
        imul, r10, p2
        { 0xF3, 0x4C, 0x0F, 0x38, 0xF6, 0xD0 } ;adox, r10, rax ; (Digits[Index] * Right) + Carry

        mov, rax, r10
        xor, rdx, rdx

        idiv, r11 ; Carry := Product / BIG_NUMBER_BASE

        mov, {r9[rcx * 4]} As i32*, edx

        inc, rcx
        cmp, rcx, rbx
        jne, EachDigit
    
    WhileCarry:
        cmp, rax, 0
        jz, End

        xor, rdx, rdx
        idiv, r11 ; Carry /= BIG_NUMBER_BASE

        mov, {r9[rcx * 4]} As i32*, edx
        inc, rcx
    
    End:
        mov, {r15 + BigNumber.DigitCount} As i32*, ecx
        mov, rax, r15

        leave
        ret
    }

