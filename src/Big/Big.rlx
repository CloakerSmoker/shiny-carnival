    define void _Decrement(i64 Value) {
        if (this->Constant) {
            Throw('shit2')
        }

        i32 Borrow := 0
        i64 Carry := -Value

        for (i32 Index := 0, Index < this->DigitCount, Index++) {
            i64 Difference := this->Digits[Index] + Carry

            Carry := Difference / BIG_NUMBER_BASE
            Difference %= BIG_NUMBER_BASE

            if (Difference < 0) {
                this->Digits[Index] := (Difference + BIG_NUMBER_BASE) As i32
                Carry -= 1
            }
            else {
                this->Digits[Index] := Difference As i32
            }
        }
        
        this->Shrink()
    }

    static BigNumber* BaseSubtract(BigNumber* Left, BigNumber* Right, BigNumber* Result) {
        i64 Borrow := 0

        for (i32 Index := 0, Index < Right->DigitCount, Index++) {
            i64 L := Left->Digits[Index]
            i64 R := Right->Digits[Index]

            i64 Difference := L - Borrow - R

            if (Difference < 0) {
                Difference += BIG_NUMBER_BASE
                Borrow := 1
            }
            else {
                Borrow := 0
            }

            ;Print("%i - %i - %i = %i\n", L, Borrow, R, Difference)

            Result->Digits[Index] := Difference As i32
        }

        ;Print("Borrow: %i\n", Borrow)

        for (i32 Index := Right->DigitCount, Index < Left->DigitCount, Index++) {
            i64 Difference := Left->Digits[Index] - Borrow

            if (Difference < 0) {
                Difference += BIG_NUMBER_BASE
            }
            else {
                Result->Digits[Index++] := Difference As i32
                break
            }

            Result->Digits[Index] := Difference As i32
        }

        while (Index < Left->DigitCount) {
            Result->Digits[Index] := Left->Digits[Index]
            Index++
        }

        Left->Release()
        Right->Release()

        return Result->Shrink()
    }

    static BigNumber* BaseSubtract(BigNumber* Left, BigNumber* Right) {
        BigNumber* Result := BigNumber:WithDigits(Left->DigitCount)
        
        return self:BaseSubtract(Left, Right, Result)
    }

    static BigNumber* FastBaseSubtract(BigNumber* Left, BigNumber* Right, BigNumber* Result) {
        i64 Borrow := 0

        asm (
            rbx := 0x1_0000_0000 - Right->DigitCount,
            rdi := Right->Digits,
            rsi := Left->Digits,
            rdx := Result->Digits,
            r8 := &Borrow
        ) {
            clc
            xor, eax, eax
            mov, r15d, 16777216
            mov, r12d, 1

        EachDigit:
            mov, r14d, {rsi[rcx * 4]} As i32*
            sbb, r14d, {rdi[rcx * 4]} As i32*

            { 0x0F, 0x92, 0xC0 } ; setc, a

            mov, r13d, 0
            cmovc, r13d, r15d

            { 0xF3, 0x45, 0x0F, 0x38, 0xF6, 0xF5 } ; adox, r14d, r13d

            mov, {rdx[rcx * 4]} As i32*, r14d

            inc, rcx

            { 0xF3, 0x41, 0x0F, 0x38, 0xF6, 0xDC } ; adox, ebx, r12d
            jno, EachDigit

            mov, {r8} As i32*, eax
        }

        ;Print("Bottor: %i\n", Borrow)

        for (i32 Index := Right->DigitCount, Index < Left->DigitCount, Index++) {
            i64 Difference := Left->Digits[Index] - Borrow

            if (Difference < 0) {
                Difference += BIG_NUMBER_BASE
            }
            else {
                Result->Digits[Index++] := Difference As i32
                break
            }

            Result->Digits[Index] := Difference As i32
        }

        while (Index < Left->DigitCount) {
            Result->Digits[Index] := Left->Digits[Index]
            Index++
        }

        Left->Release()
        Right->Release()

        return Result->Shrink()
    }

    static BigNumber* FastBaseSubtract(BigNumber* Left, BigNumber* Right) {
        BigNumber* Result := BigNumber:WithDigits(Left->DigitCount)
        
        return self:FastBaseSubtract(Left, Right, Result)
    }

    static BigNumber* FastSubtract(BigNumber* Left, BigNumber* Right) {
        if (Left->Negative != Right->Negative) {
            return Left->Add(Right->Negate())
        }

        ComparisonResult Comparison := BigNumber:CompareAbsoluteValue(Left, Right)

        if (Comparison = ComparisonResult::Equal || Comparison = ComparisonResult::Greater) {
            return self:FastBaseSubtract(Left, Right)
        }
        else {
            BigNumber* Result := self:FastBaseSubtract(Right, Left)
            Result->Negative := !Left->Negative
            return Result
        }
    }

    static BigNumber* Subtract(BigNumber* Left, BigNumber* Right) {
        if (Left->Negative != Right->Negative) {
            return Left->Add(Right->Negate())
        }

        ComparisonResult Comparison := BigNumber:CompareAbsoluteValue(Left, Right)

        if (Comparison = ComparisonResult::Equal || Comparison = ComparisonResult::Greater) {
            return self:FastBaseSubtract(Left, Right)
        }
        else {
            BigNumber* Result := self:FastBaseSubtract(Right, Left)
            Result->Negative := !Left->Negative
            return Result
        }
    }

